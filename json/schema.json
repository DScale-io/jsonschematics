{
  "version": "1",
  "fields": [{
    "depends_on": [],
    "target_key": "user.profile.name.first",
    "description": "",
    "validators": ["IsString", "IsRequired", "MaxLengthAllowed"],
    "operators": ["Capitalize"],
    "constants": {
      "MaxLengthAllowed": {
        "attributes": {
          "max": 2
        },
        "err": "user's first name should have maximum 2 characters"
      }
    }
  }, {
    "depends_on": [],
    "target_key": "user.profile.name.last",
    "description": "",
    "validators": ["IsString", "IsRequired", "MinLengthAllowed"],
    "constants": {
      "MinLengthAllowed": {
        "attributes": {
          "min": 10,
          "err": "user's last name should have minimum 10 characters"
        }
      }
    }
  }, {
    "depends_on": [],
    "target_key": "user.profile.age",
    "description": "",
    "validators": ["IsNumber", "IsRequired", "MaxAllowed"],
    "constants": {
      "MaxAllowed": {
        "attributes": {
          "max": 20,
          "err": "user's age is greater than 20"
        }
      }
    }
  }, {
    "depends_on": ["user.personalInfo.firstname", "user.personalInfo.lastname"],
    "target_key": "user.profile.email",
    "description": "",
    "validators": ["IsString", "IsRequired","IsEmail"],
    "constants": {
      "IsEmail": {
        "err": "User's Email Address Is Invalid"
      }
    }
  }]
}